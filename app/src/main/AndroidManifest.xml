<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="vdd.smart.techparamssample">

    <uses-permission android:name="android.permission.GET_PACKAGE_SIZE" />
    <application
            android:name=".TechParamsSampleApp"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".crash.vm.VMCrashActivity" />
        <activity android:name=".crash.native.NativeCrashActivity" />
        <activity android:name=".threads.ThreadsActivity" />
        <activity android:name=".ram.RAMActivity" />
        <activity android:name=".storage.StorageActivity" />
        <activity android:name=".battery.BatteryActivity" />
        <activity android:name=".fps.FrameRateActivity" />
        <activity android:name=".bundle.BundleActivity" />
    </application>

</manifest>